{
    "swift.path.sourcekitLsp": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
    "swift.path.swift": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/swift",
    "swift.buildArguments": [
        "-target", "x86_64-apple-macosx14.0",
        "-I", "${workspaceFolder}/AdvanceFilter"
    ],
    "swift.autoGenerateLaunchConfigurations": true,
    "swift.diagnostics.enableOnEdit": true,
    "swift.path": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin",
    "swift.swiftEnvironmentVariables": {
        "DEVELOPER_DIR": "/Applications/Xcode.app",
        "TOOLCHAINS": "XcodeDefault",
        "TOOLCHAIN_DIR": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain"
    },
    "sourcekit-lsp.toolchainPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain",
    "sourcekit-lsp.serverArguments": [
        "-Xswiftc", "-target",
        "-Xswiftc", "x86_64-apple-macosx14.0",
        "-Xswiftc", "-I${workspaceFolder}/AdvanceFilter"
    ],
    "swift.sourceKit.searchPaths": [
        "${workspaceFolder}/AdvanceFilter",
        "${workspaceFolder}/AdvanceFilter/Views",
        "${workspaceFolder}/AdvanceFilter/Models",
        "${workspaceFolder}/AdvanceFilter/extensions"
    ],
    "files.associations": {
        "*.swift": "swift"
    },
    "editor.semanticHighlighting.enabled": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "editor.suggestSelection": "first",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggest.showKeywords": true,
    "go.toolsManagement.checkForUpdates": "local",
    "go.useLanguageServer": true,
    "swift.buildPath": "${workspaceFolder}/build",
    "swift.targets": [
        {
            "name": "AdvanceFilter",
            "path": "${workspaceFolder}/AdvanceFilter.xcworkspace"
        }
    ]
}